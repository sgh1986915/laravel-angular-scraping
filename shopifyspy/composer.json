{
	"require": {
		"laravel/framework": "4.2.*",
		"cartalyst/sentry": "~2",
		"guzzlehttp/guzzle": "4.x",
		"fabpot/goutte": "~2.0",
		"alaouy/youtube": "dev-master",
		"dts/ebay-sdk-finding": "dev-master",
		"exeu/apai-io": "1.x"
	},
	"require-dev": {
		"way/generators": "~2.6",
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/Libraries",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
			"Authority": "app/"
		}
	},
	"scripts": {
		"post-update-cmd": "php artisan optimize"
	},
	"minimum-stability": "stable"
}
